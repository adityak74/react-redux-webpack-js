{
  "name": "react-redux-webpack-js-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.5.1",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.3.2",
    "@testing-library/user-event": "7.1.2",
    "mini-svg-data-uri": "1.4.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.3",
